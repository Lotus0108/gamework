<h2>Results index</h2>

<table class="table table-hover table-inverse">
  <% if @range == "Public" %>
    <tbody>
      <% @users.each do |public| %>
        <tr>
          <td><%= public.name %></td>
        </tr>
      <% end %>
    </tbody>
  <% else %>
    <tbody>
      <% @books.each do |item| %>
        <tr>
          <td><%= item.title %></td>
          <td><%= item.body %></td>
        </tr>
      <% end %>
    </tbody>
  <% end %>
</table>